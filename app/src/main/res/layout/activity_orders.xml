<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:geekui="http://schemas.android.com/apk/res-auto"
    tools:context="shopcityshop.purplecommerce.com.interpriseapp.com.interpriseapp.OrdersActivity">




    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/interprise_welcome_image"
            android:padding="@dimen/_40sdp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#EEEEEE"
            android:alpha="0.9"></LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <LinearLayout
            android:id="@+id/ll_toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_40sdp"
            android:orientation="horizontal"
            android:gravity="center">


         </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <views.textview.TypefaceTextView
                    android:id="@+id/txt_previous"
                    geekui:customTypeface="fonts/CoreSansDRegular.otf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:gravity="center"
                    android:textSize="@dimen/_14sdp"
                    android:text="Previous"
                    android:layout_weight="1"
                    android:padding="@dimen/_3sdp"/>

                <views.textview.TypefaceTextView
                    android:id="@+id/ordr_count_page"
                    geekui:customTypeface="fonts/CoreSansDRegular.otf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:gravity="center"
                    android:textSize="@dimen/_14sdp"
                    android:text=""
                    android:layout_weight="0.8"
                    android:padding="@dimen/_3sdp"/>

                <views.textview.TypefaceTextView
                    android:id="@+id/txt_next"
                    geekui:customTypeface="fonts/CoreSansDRegular.otf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:gravity="center"
                    android:textSize="@dimen/_14sdp"
                    android:text="Next"
                    android:padding="@dimen/_3sdp"
                    android:layout_weight="1"/>


            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/grey"/>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="@dimen/_5sdp"
                android:visibility="gone">

                <views.textview.TypefaceTextView
                    geekui:customTypeface="fonts/CoreSansDBold.otf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:gravity="center"
                    android:textSize="@dimen/_14sdp"
                    android:text="Date"
                    android:layout_weight="0.8"/>

                <views.textview.TypefaceTextView
                    geekui:customTypeface="fonts/CoreSansDBold.otf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:gravity="center"
                    android:textSize="@dimen/_14sdp"
                    android:text="Order#"
                    android:layout_weight="0.8"/>

                <views.textview.TypefaceTextView
                    geekui:customTypeface="fonts/CoreSansDBold.otf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:gravity="center"
                    android:textSize="@dimen/_14sdp"
                    android:text="Amount"
                    android:layout_weight="0.8"/>

                <views.textview.TypefaceTextView
                    geekui:customTypeface="fonts/CoreSansDBold.otf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:gravity="center"
                    android:textSize="@dimen/_14sdp"
                    android:text="Status"
                    android:layout_weight="0.8"/>

                <views.textview.TypefaceTextView
                    geekui:customTypeface="fonts/CoreSansDBold.otf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:gravity="center"
                    android:textSize="@dimen/_14sdp"
                    android:text=""
                    android:layout_weight="1"/>



            </LinearLayout>



            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fillViewport="true"
                android:layout_marginBottom="@dimen/_50sdp">


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >




                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">




                    <LinearLayout
                        android:id="@+id/ll_orders_add_parent"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:padding="@dimen/_10sdp"
                        >




                    </LinearLayout>





                </LinearLayout>








            </FrameLayout>


            </ScrollView>


            </LinearLayout>




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_margin="@dimen/_10sdp"
            android:elevation="@dimen/_1sdp"
            android:background="@drawable/primary_back_round"
            android:baselineAligned="false">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/_2sdp"
                android:layout_weight="0.5">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <!--<LinearLayout-->
                        <!--android:layout_width="wrap_content"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:background="@color/white">-->

                    <Spinner
                        android:id="@+id/spinner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"/>

                    <!--</LinearLayout>-->

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="@dimen/_3sdp"
                        android:layout_gravity="center">


                <EditText
                    android:id="@+id/ed_search_order"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Enter Order No"
                    android:textColorHint="@color/muted_white"
                    android:background="@null"
                    android:paddingLeft="@dimen/_4sdp"
                    android:textColor="@color/white"
                    android:inputType="number"/>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:background="@color/white"/>

                    </LinearLayout>

                </LinearLayout>



            </LinearLayout>


            <views.textview.TypefaceTextView
                android:id="@+id/txt_find"
                geekui:customTypeface="fonts/CoreSansDRegular.otf"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:layout_weight="1"
                android:textSize="@dimen/_18sdp"
                android:text="FIND"
                android:gravity="center"
                android:layout_gravity="center"
                android:padding="@dimen/_2sdp"
                android:layout_marginLeft="@dimen/_5sdp"/>



        </LinearLayout>



    </FrameLayout>





</LinearLayout>
